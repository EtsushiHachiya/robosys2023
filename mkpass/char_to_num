#!/usr/bin/python3

alphabet_to_number = {'a': 1, 'b': 2, 'c': 3, 'd': 4, 'e': 5, 'f': 6, 'g': 7, 'h': 8, 'i': 9, 'j': 0, 'k': 1, 'l': 2, 'm': 3, 'n': 4, 'o': 5, 'p': 6, 'q': 7, 'r': 8, 's': 9, 't': 0, 'u': 1, 'v': 2, 'w': 3, 'x': 4, 'y': 5, 'z': 6}

import sys

for n in sys.argv[1:]:
    char = n
    number = alphabet_to_number[char]
    print(number, end=' ')

print(' ')

